{
    "name": "Device Tracker",
    "version": "0.9.13",
    "slug": "device_tracker",
    "description": "Track when devices connect to your network and get notifications",
    "url": "https://github.com/Smekl/Device-Tracker",
    "arch": ["amd64", "armv7"],
    "startup": "application",
    "boot": "auto",
    "options": {
        "entities": [{
            "entity": "device_tracker.placeholder",
            "name": "Friendly Name",
            "mac": "aa:bb:cc:11:22:33"
        }],
        "timeout": 5,
        "absence_timeout": 180,
        "asus": false,
        "nodes": [],
        "node_change_time": 3,
        "ip": "192.168.50.1",
        "user": "admin",
        "key": "/ssl/asus/id_rsa",
        "debug": false
    },
    "schema": {
        "entities": [
            {
                "entity": "str",
                "name": "str",
                "mac": "str"
            }
        ],
        "timeout": "int",
        "absence_timeout": "int",
        "asus": "bool",
        "ip": "str",
        "nodes": [
            {
                "ip": "str"
            }
        ],
        "node_change_time": "int",
        "user": "str",
        "key": "str",
        "debug": "bool"
    },
    "host_network": true,
    "ingress": false,
    "homeassistant_api": true,
    "init":false,
    "map": ["ssl:ro"]
}
