{
    "name": "Device Tracker",
    "version": "0.7.24",
    "slug": "device_tracker",
    "description": "Track when devices connect to your network and get notifications",
    "url": "https://github.com/Smekl/Device-Tracker",
    "arch": ["amd64", "armv7"],
    "startup": "application",
    "boot": "auto",
    "options": {
        "entities": {
            "name": "str",
            "mac": "str",
        },
        "timeout": 5
    },
    "schema": {
        "entities": {
            "name": "str",
            "mac": "str",
        },
        "timeout": "int"
    },
    "host_network": true,
    "ingress": true,
    "homeassistant_api": true,
    "init":false,
    "map": ["ssl:ro"]
}
